{
  "name": "fb-downloader-scrapper",
  "version": "3.0.0",
  "description": "Scrap easily video info and direct url data from Facebook",
  "main": "dist/esm/facebook.js",
  "types": "dist/types/facebook.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "postbuild": "esm2cjs --in dist/esm --out dist/cjs -l error"
  },
  "exports": {
    ".": {
      "require": {
        "default": "./dist/cjs/facebook.js"
      },
      "import": {
        "default": "./dist/esm/facebook.js"
      },
      "types": "./dist/types/facebook.d.ts"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victorsouzaleal/fb-downloader-scrapper.git"
  },
  "keywords": [
    "fb",
    "downloader",
    "scrapper",
    "url",
    "download",
    "facebook"
  ],
  "author": "Victorsouzaleal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorsouzaleal/fb-downloader-scrapper/issues"
  },
  "homepage": "https://github.com/victorsouzaleal/fb-downloader-scrapper#readme",
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@alcalzone/esm2cjs": "^1.4.1",
    "@types/node": "^22.13.1",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.3"
  }
}
